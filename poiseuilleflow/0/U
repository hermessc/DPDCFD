FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
     top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            symmetryPlane;
    }
    inlet
    {
	type		pressureInletVelocity;
	value		uniform (1 0 0);
    }
    outlet
    {
	type		inletOutlet;
	inletValue uniform (1 0 0);
	value 		uniform (1 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}
// ************************************************************************* //
